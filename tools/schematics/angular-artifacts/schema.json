{
    "$schema": "http://json-schema.org/schema",
    "id": "Senses2SchematicsAngularComponent",
    "title": "Angular Component Options Schema",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "description": "The name of the component.",
            "$default": {
                "$source": "argv",
                "index": 0
            }
        },
        "feature": {
            "type": "string",
            "description": "The feature owning this component."
        },
        "page": {
            "type": "string",
            "description": "The page owning this component."
        }
    },
    "required": [],
    "additionalProperties": true
}
