{
    "$schema":
        "../node_modules/@angular-devkit/schematics/collection-schema.json",
    "name": "senses2-schematics-collection",
    "version": "0.0.1",
    "extends": ["@nrwl/schematics"],
    "schematics": {
        "component": {
            "aliases": ["c"],
            "factory": "./angular-artifacts/index#angularComponent",
            "description": "Create an Angular component.",
            "schema": "./angular-artifacts/schema.json"
        },
        "pipe": {
            "aliases": ["p"],
            "factory": "./angular-artifacts/index#angularPipe",
            "description": "Create an Angular pipe.",
            "schema": "./angular-artifacts/schema.json"
        },
        "service": {
            "aliases": ["s"],
            "factory": "./angular-artifacts/index#angularService",
            "description": "Create an Angular service.",
            "schema": "./angular-artifacts/schema.json"
        },
        "guard": {
            "aliases": ["g"],
            "factory": "./angular-artifacts/index#angularGuard",
            "description": "Create a guard.",
            "schema": "./angular-artifacts/schema.json"
        },
        "class": {
            "aliases": ["cl"],
            "factory": "./angular-artifacts/index#angularClass",
            "description": "Create a class.",
            "schema": "./angular-artifacts/schema.json"
        },
        "directive": {
            "aliases": ["d"],
            "factory": "./angular-artifacts/index#angularDirective",
            "description": "Create an Angular directive.",
            "schema": "./angular-artifacts/schema.json"
        },
        "enum": {
            "aliases": ["e"],
            "factory": "./angular-artifacts/index#angularEnum",
            "description": "Create an enumeration.",
            "schema": "./angular-artifacts/schema.json"
        }
    }
}
